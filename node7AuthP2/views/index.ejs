<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-zinc-800">
  <div class="w-full bg-zinc-400 min-h-10 p-10 text-white font-bold">
    <form action="/create" method="post">
        <input class="p-1 m-1 rounded-md bg-zinc-700 pl-5" type="text" placeholder="userName" name="userName">
        <input class="p-1 m-1 rounded-md bg-zinc-700 pl-5" type="email" placeholder="email" name="email">
        <input class="p-1 m-1 rounded-md bg-zinc-700 pl-5" type="password" placeholder="password" name="password">
        <input class="p-1 m-1 rounded-md bg-zinc-700 pl-5" type="number" placeholder="age" name="age">
        <input class="border-2 border-black p-1 cursor-pointer rounded-md" type="submit" value="Create user ">
    </form>
  </div>
</body>
</html>